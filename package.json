{
  "name": "front-starter",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "animate.css": "^3.5.2",
    "autoprefixer": "7.1.6",
    "axios": "^0.16.2",
    "bootstrap": "^4.0.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "0.11.2",
    "fork-ts-checker-webpack-plugin": "^0.2.8",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "jest": "20.0.4",
    "lodash": "^4.17.4",
    "moment": "^2.19.1",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "react": "^16.2.0",
    "react-dev-utils": "4.2.1",
    "react-dom": "^16.2.0",
    "react-redux": "5.0.4",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "5.0.0-alpha.6",
    "redux": "^3.7.2",
    "redux-saga": "^0.15.6",
    "reselect": "^3.0.1",
    "source-map-loader": "^0.2.1",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "ts-jest": "^20.0.7",
    "ts-loader": "^2.3.7",
    "tslint": "^5.7.0",
    "tslint-loader": "^3.5.3",
    "tslint-react": "^3.2.0",
    "typescript": "^2.6.2",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom --json --outputFile=.storybook/.jest-test-results.json",
    "test:coverage": "node scripts/test.js --coverage",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "gen:component": "plop"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.3.11",
    "@storybook/addon-links": "^3.3.11",
    "@storybook/react": "^3.3.11",
    "@types/enzyme-adapter-react-16": "^1.0.1",
    "@types/expect": "^1.20.2",
    "@types/history": "4.5.1",
    "@types/jest": "^22.0.1",
    "@types/lodash": "^4.14.80",
    "@types/node": "8.0.26",
    "@types/react": "15.0.35",
    "@types/react-dates": "^12.1.6",
    "@types/react-dom": "^15.5.1",
    "@types/react-redux": "4.4.45",
    "@types/react-router": "^4.0.15",
    "@types/react-router-dom": "4.0.6",
    "@types/react-router-redux": "5.0.3",
    "@types/react-test-renderer": "^16.0.0",
    "@types/webpack": "2.2.15",
    "@types/webpack-env": "1.13.0",
    "awesome-typescript-loader": "^3.4.1",
    "chai": "^4.1.2",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.1",
    "expect": "^22.0.1",
    "jquery": "^3.3.1",
    "json-server": "^0.12.0",
    "node-sass": "^4.7.2",
    "plop": "^1.9.1",
    "react-addons-test-utils": "^15.6.2",
    "react-hot-loader": "^3.1.3",
    "react-test-renderer": "^16.2.0",
    "sass-loader": "^6.0.6",
    "sinon": "^4.2.2",
    "storybook-addon-jest": "0.0.1",
    "storybook-addon-specifications": "^2.1.1",
    "ts-jest": "^20.0.14"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/jest/jest.setup.ts",
      "<rootDir>/config/polyfills.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.ts?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).ts?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/config/jest/typescriptTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|ts|tsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.(css|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "mjs",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "web.js",
      "js",
      "web.jsx",
      "jsx",
      "json",
      "node"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.test.json"
      }
    }
  },
  "testPathIgnorePatterns": [
    "<rootDir>/config/jest/jest.setup.ts"
  ],
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
