{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
      "ordered-imports": false,
      "quotemark": ["single", "jsx-single"],
      "object-literal-sort-keys": [
        false
      ],
      "jsdoc-format": false,
      "interface-name": false,
      "member-access": false,
      "curly": false,
      "max-line-length": false,
      "no-string-literal": false,
      "forin": false
    },
    "rulesDirectory": []
}
